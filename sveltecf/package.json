{
    "name": "sveltecf",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && pnpm run lint",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "pnpm run -F sveltecf --parallel \"/lint:.+/\"",
        "lint:biome": "biome lint .",
        "lint:eslint": "eslint --ext .js,.ts,.svelte .",
        "test": "pnpm run -F sveltecf --parallel \"/test:.+/\"",
        "test:unit": "vitest run",
        "test:integration": "playwright test",
        "pages:dev": "wrangler pages dev --compatibility-date=2023-10-25 --proxy 5173 -- pnpm run dev",
        "pages:deploy": "pnpm run  build && wrangler pages deploy .svelte-kit/cloudflare",
        "clean": "pnpm run -F sveltecf --parallel \"/clean:.+/\"",
        "clean:sveltekit": "rm -rf ./.svelte-kit"
    },
    "devDependencies": {
        "@playwright/test": "^1.40.1",
        "@sveltejs/adapter-auto": "^2.1.1",
        "@sveltejs/adapter-cloudflare": "^2.3.4",
        "@sveltejs/kit": "^1.30.3",
        "@types/katex": "^0.16.7",
        "eslint-plugin-svelte": "^2.35.1",
        "prettier-plugin-svelte": "^2.10.1",
        "svelte": "^4.2.8",
        "svelte-check": "^3.6.2",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
        "uniproc": "workspace:*",
        "vite": "^4.5.1",
        "vitest": "^0.32.4",
        "wrangler": "^3.22.1"
    },
    "type": "module",
    "dependencies": {
        "katex": "^0.16.9"
    }
}
