{
    "name": "sveltecf",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && pnpm run lint",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "pnpm run -F sveltecf --parallel \"/lint:.+/\"",
        "lint:eslint": "ESLINT_USE_FLAT_CONFIG=false eslint --ext .js,.ts,.svelte .",
        "test": "pnpm run -F sveltecf --parallel \"/test:.+/\"",
        "test:unit": "vitest run",
        "test:e2e": "playwright test",
        "pages:dev": "wrangler pages dev --compatibility-date=2023-10-25 --proxy 5173 -- pnpm run dev",
        "pages:deploy": "pnpm run  build && wrangler pages deploy .svelte-kit/cloudflare",
        "clean": "pnpm run -F sveltecf --parallel \"/clean:.+/\"",
        "clean:sveltekit": "rm -rf ./.svelte-kit"
    },
    "devDependencies": {
        "@iconify/svelte": "^4.2.0",
        "@playwright/test": "^1.50.1",
        "@sveltejs/adapter-auto": "^4.0.0",
        "@sveltejs/adapter-cloudflare": "^5.0.2",
        "@sveltejs/kit": "^2.17.1",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@types/katex": "^0.16.7",
        "date-fns": "^4.1.0",
        "eslint-plugin-svelte": "^2.46.1",
        "open-props": "^1.7.12",
        "postcss": "^8.5.2",
        "postcss-jit-props": "^1.0.15",
        "prettier-plugin-svelte": "^3.3.3",
        "ress": "^5.0.2",
        "rollup-plugin-visualizer": "^5.14.0",
        "shiki": "^2.3.2",
        "svelte": "^5.19.9",
        "svelte-check": "^4.1.4",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3",
        "uniproc": "workspace:*",
        "vite": "^6.1.5",
        "vitest": "^3.0.5",
        "wrangler": "^3.107.3"
    },
    "type": "module",
    "dependencies": {
        "katex": "^0.16.21"
    }
}
