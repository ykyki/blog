{
    "name": "sveltecf",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && pnpm run lint",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "pnpm run -F sveltecf --parallel \"/lint:.+/\"",
        "lint:eslint": "ESLINT_USE_FLAT_CONFIG=false eslint --ext .js,.ts,.svelte .",
        "test": "pnpm run -F sveltecf --parallel \"/test:.+/\"",
        "test:unit": "vitest run",
        "test:e2e": "playwright test",
        "pages:dev": "wrangler pages dev --compatibility-date=2023-10-25 --proxy 5173 -- pnpm run dev",
        "pages:deploy": "pnpm run  build && wrangler pages deploy .svelte-kit/cloudflare",
        "clean": "pnpm run -F sveltecf --parallel \"/clean:.+/\"",
        "clean:sveltekit": "rm -rf ./.svelte-kit"
    },
    "devDependencies": {
        "@iconify/svelte": "^5.0.0",
        "@playwright/test": "^1.52.0",
        "@sveltejs/adapter-auto": "^6.0.1",
        "@sveltejs/adapter-cloudflare": "^7.0.4",
        "@sveltejs/kit": "^2.21.3",
        "@sveltejs/vite-plugin-svelte": "^5.1.0",
        "@types/katex": "^0.16.7",
        "date-fns": "^4.1.0",
        "eslint-plugin-svelte": "^3.9.1",
        "open-props": "^1.7.15",
        "postcss": "^8.5.4",
        "postcss-jit-props": "^1.0.16",
        "prettier-plugin-svelte": "^3.4.0",
        "ress": "^5.0.2",
        "rollup-plugin-visualizer": "^6.0.3",
        "shiki": "^3.6.0",
        "svelte": "^5.33.18",
        "svelte-check": "^4.2.1",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3",
        "uniproc": "workspace:*",
        "vite": "^6.3.5",
        "vitest": "^3.2.3",
        "wrangler": "^4.19.1"
    },
    "type": "module",
    "dependencies": {
        "katex": "^0.16.22"
    }
}
